require(arules)
data(package="arules")
#Groc<-read.transactions("groceries.csv",sep=",")
data("Groceries")
Groceries  
str(Groceries)
class(Groceries)
summary(Groceries)
inspect(Groceries[1:5])
itemFrequency(Groceries[,1])
itemFrequency(Groceries[,1:5])
itemFrequencyPlot(Groceries,support=0.05)
itemFrequencyPlot(Groceries,topN=10)
model1<-apriori(Groceries)
model1<-apriori(Groceries,parameter = list(support=0.007,confidence=0.25,minlen=2))
model1
summary(model1)
inspect(model1[1:10])
inspect(sort(model1,by="lift")[1:10])
